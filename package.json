{
  "name": "nextjs",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "alasql": "^0.6.5",
    "next": "10.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.30.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "puppeteer": "^5.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "next dev",
    "lint": "eslint src --max-warnings=0",
    "serve": "next start",
    "build": "next build",
    "export": "next export",
    "test_start_server": "next start -p 9754 &",
    "test_run_tests": "jest --verbose || npm run test_stop_server",
    "test_stop_server": "kill -9 $(lsof -i TCP:9754 | grep LISTEN | awk '{print $2}')",
    "test": "npm run build && npm run test_start_server && npm run test_run_tests && npm run test_stop_server"
  }
}
